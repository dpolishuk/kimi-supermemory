{
  "name": "kimi-supermemory-mcp",
  "version": "1.0.0",
  "description": "MCP server for Supermemory integration with Kimi CLI",
  "type": "module",
  "main": "dist/server.cjs",
  "bin": {
    "kimi-supermemory-mcp": "dist/server.cjs"
  },
  "scripts": {
    "build": "esbuild src/server.js --bundle --platform=node --target=node18 --format=cjs --outfile=dist/server.cjs --minify",
    "dev": "node src/server.js",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "kimi-cli",
    "mcp",
    "supermemory",
    "memory",
    "ai"
  ],
  "author": "Supermemory",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "supermemory": "^4.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "esbuild": "^0.25.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
